<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Laboratorio4"
        tools:targetApi="31">
        <activity
            android:name=".InfoCampus"
            android:exported="false"
            android:label="@string/title_activity_info_campus"
            android:theme="@style/Theme.Laboratorio4" />
        <activity
            android:name=".EmergencyProf"
            android:exported="false"
            android:label="@string/title_activity_emergency_prof"
            android:theme="@style/Theme.Laboratorio4" />
        <activity
            android:name=".ConfigProf"
            android:exported="false"
            android:label="@string/title_activity_config_prof"
            android:theme="@style/Theme.Laboratorio4" />
        <activity
            android:name=".MyProf"
            android:exported="false"
            android:label="@string/title_activity_my_prof"
            android:theme="@style/Theme.Laboratorio4" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Laboratorio4">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>